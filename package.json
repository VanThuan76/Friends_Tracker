{
  "name": "friendstracker",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios_develop": "react-native run-ios --simulator=\"iPhone 15 Pro\"",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "type-check": "tsc",
    "test:report": "jest --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --coverage",
    "pod-clean": "cd ios && rm -rf Pods Podfile.lock",
    "pod-install": "cd ios && pod install && cd ..",
    "pod-install-update": "cd ios && pod install --repo-update && cd ..",
    "postinstall": "npx patch-package",
    "gradle-clean": "cd android && ./gradlew clean && cd ..",
    "gradle-build": "cd android && cd app && rm -rf build && cd .. && ./gradlew build --info && cd ..",
    "gradle-signing-report": "cd android && ./gradlew signingReport && cd .."
  },
